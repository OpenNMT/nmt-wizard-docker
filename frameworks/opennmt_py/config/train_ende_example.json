{
    "source": "en",
    "target": "de",
    "tokenization": {
        "mode": "aggressive",
        "joiner_annotate": true
    },
    "options": {
        "config": {
            "preprocess": {
            },
            "train": {
                "batch_size": 64,
                "optim": "sgd",
                "dropout": 0.3,
                "learning_rate": 1.0,
                "src_word_vec_size": 512,
                "tgt_word_vec_size": 512,
                "encoder_type": "rnn",
                "decoder_type": "rnn",
                "layers": 2,
                "enc_layers": 2,
                "dec_layers": 2,
                "rnn_size": 512
            },
            "trans": {
                "replace_unk": "true"
            }
        }
    }
}
